    "vite": "^5.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@supabase/gotrue-js": "^2.84.0",
    "@supabase/supabase-js": "^2.84.0",
    "@types/react-router-dom": "^5.3.3",
    "lottie-react-native": "^7.3.4",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.9.6"
  }
}
