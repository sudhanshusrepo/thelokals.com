{
    "compilerOptions": {
        "target": "es2021",
        "lib": [
            "es2021",
            "webworker"
        ],
        "module": "es2022",
        "moduleResolution": "node",
        "types": [
            "@cloudflare/workers-types"
        ],
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noEmit": true,
        "strict": true
    },
    "include": [
        "src"
    ]
}