{
    "name": "@thelocals/platform-core",
    "version": "0.0.0",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./services/*": "./src/services/*.ts",
        "./types": "./src/types.ts",
        "./theme/colors": "./src/theme/colors.ts",
        "./*": "./src/*.ts"
    },
    "typesVersions": {
        "*": {
            "types": [
                "./src/types.ts"
            ],
            "services/*": [
                "./src/services/*"
            ],
            "theme/colors": [
                "./src/theme/colors.ts"
            ],
            "*": [
                "./src/*"
            ]
        }
    },
    "scripts": {
        "lint": "eslint . --ext .ts,.tsx",
        "type-check": "tsc --noEmit"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "dependencies": {
        "@google/generative-ai": "^0.11.0",
        "@supabase/supabase-js": "^2.43.4",
        "@tanstack/react-query": "^5.90.20",
        "@thelocals/platform-config": "*",
        "clsx": "^2.1.1",
        "lucide-react": "^0.378.0",
        "set-cookie-parser": "^2.6.0",
        "tailwind-merge": "^2.3.0",
        "zod": "^3.22.3"
    },
    "devDependencies": {
        "@types/google.maps": "^3.58.1",
        "@types/node": "^20.12.12",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "typescript": "^5.4.5",
        "vitest": "^4.0.18"
    }
}
